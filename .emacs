perstar-mode declaration;(require 'org-superstar);(add-hook 'org-mode-hook (lambda () (org-superstar-mode 1)));; vertico(use-package vertico  :init  (vertico-mode));; bibtex-completion(use-package bibtex-completion  :config  (setq   bibtex-completion-bibliography "/home/user/org/bibliography/master.bib"   bibtex-completion-library-path "/home/user/Zotero/storage/"   bibtex-completion-pdf-field "file"));; org-ref(use-package org-ref  :config  (setq org-ref-get-pdf-filename-function 'org-ref-get-pdf-filename-helm-bibtex	org-ref-default-bibiliography (list "/home/user/org/bibliography/master.bib")	org-ref-notes-function 'orb-edit-notes));; org-roam(use-package org-roam  :ensure t  :init  (setq org-roam-v2-ack t)  :custom  (org-roam-directory "/home/user/org/org-roam/")  :config  (org-roam-bibtex-mode +1)  (setq org-roam-mode-section-functions	(list #'org-roam-backlinks-section	      #'org-roam-reflinks-section	      ))  (add-to-list 'display-buffer-alist             '("\\*org-roam\\*"               (display-buffer-in-direction)               (direction . right)               (window-width . 0.33)               (window-height . fit-window-to-buffer)))  (setq org-roam-capture-templates	'(("d" "default" plain   "%?"   :target   (file+head "%<%Y%m%d%H%M%S>-${slug}.org"	      "* ${title}:properties::roam_refs: :end::logbook:(placeholder drawer, reminder to clock in work at the edges of the document tree):end::export:#+startup: latexpreview#+setupfile: https://fniessen.github.io/org-html-themes/org/theme-readtheorg.setup:end:")   :unnarrowed t)	  ("r" "bibliography reference" plain "%?"         :target         (file+head "%<%y%m%d%h%m%s>-${slug}.org"		    "* ${title}:properties::roam_refs::noter_document: ${file}:noter_page::end::logbook:(placeholder drawer, reminder to clock in work at the edges of the document tree):end::export:#+startup: latexpreview#+setupfile: https://fniessen.github.io/org-html-themes/org/theme-readtheorg.setup:end:")         :unnarrowed t)))  (org-roam-db-autosync-mode)  (org-roam-setup));; org-roam-bibtex(use-package org-roam-bibtex  :after org-roam  :config  (setq orb-preformat-keywords      '("citekey" "title" "url" "author-or-editor" "keywords" "file")      orb-process-file-keyword t      orb-file-field-extensions '("pdf"))); org-roam-ui(add-to-list 'load-path "~/.emacs.d/private/org-roam-ui")(load-library "org-roam-ui");; org-noter(use-package org-noter  :after (:any org pdf-view)  :config  (setq   org-noter-notes-window-location 'other-frame   org-noter-always-create-frame nil   org-noter-hide-other nil   org-noter-notes-search-path "/home/user/org/org-roam"   )  );; org-gtd(use-package org-gtd  :after org  :demand t  :config  (setq org-gtd-directory "/home/user/org/gtd/"));; org-edna(use-package org-edna  :config  (setq org-edna-use-inheritance t)  (org-edna-mode 1)  );; org-agenda(use-package org-agenda  :config  (setq  ;; org-agenda-span 1   org-agenda-files (list "/home/user/org/gtd" "/home/user/org/org-roam")));; org-super-agenda (use-package org-super-agenda  :config  (setq org-super-agenda-groups	'(	  ;;(:name "routine"		;; :time-grid t)	  (:todo "NEXT")	  (:name "upcoming"		 :and (:todo "TODO" :not (:discard (:habit t))))	  (:name "Habits"		 :habit t)	  (:name "pro re nata"		 :todo "PRN")))  (org-super-agenda-mode 1));; pdf-tools declaration;(use-package pdf-tools ; :pin manual ; :config  ;(pdf-tools-install));; quelpa-use-package declaration(quelpa '(quelpa-use-package   :fetcher git   :url "https://github.com/quelpa/quelpa-use-package.git"))(require 'quelpa-use-package);; prism declaration(use-package prism  :quelpa  (prism :fetcher github :repo "alphapapa/prism.el"));; org-babel (for literate programming)(org-babel-do-load-languages 'org-babel-load-languages '((latex . t)));; tikz(eval-after-load "preview"  '(add-to-list 'preview-default-preamble "\\previewenvironment{tikzpicture}" t)  );; org-ql(setq org-tags-exclude-from-inheritance '("tactic" "strategy"));(use-package org-ql  ;:config  ;(org-ql-search (org-agenda-files)  ;'(and (todo "NEXT"))  ;:sort '(todo)  ;:super-groups '(		  ;(:todo "NEXT"))));; org-timer(add-to-list 'org-modules 'org-timer)(setq org-timer-default-timer 90)(add-hook 'org-clock-in-hook (lambda ()			       (if (not org-timer-current-timer)				   (org-timer-set-timer '(16)))));; org-fragtog;; (add-hook 'org-mode-hook 'org-fragtog-mode);; hydra(use-package hydra  :defer 2  :bind ("C-c c" . hydra-control/body)  )(defhydra hydra-control (:color teal                             :hint nil)  "^storage^                    ^gtd^                    ^time management^^^^^^^^^---------------------------------------------------------------_b_: buffer                 _g_: gtd capture         _I_: clock in_f_: node find              _p_: process inbox       _O_: clock out_i_: insert link            _c_: choose              _r_: roam capture           _e_: engage_V_: visualize              "  ("b" org-roam-buffer-toggle)  ("f" org-roam-node-find)  ("i" org-roam-node-insert)  ("r" org-roam-capture)  ("V" org-roam-ui-mode)  ("g" org-gtd-capture)  ("p" org-gtd-process-inbox)  ("c" org-gtd-choose)  ("e" org-gtd-engage)  ("I" org-clock-in)  ("O" org-clock-out)  ("q" quit-window "quit" :color blue)  )(setq org-latex-pdf-process      '("pdflatex -shell-escape -interaction nonstopmode -output-directory %o %f"    "bibtex %b"    "pdflatex -shell-escape -interaction nonstopmode -output-directory %o %f"    "pdflatex -shell-escape -interaction nonstopmode -output-directory %o %f"));; elfeed(use-package elfeed  :config  (setq elfeed-feeds	'("https://www.science.org/action/showFeed?type=etoc&feed=rss&jc=scirobotics" ;; Science Robotics	  "https://ieeexplore.ieee.org/rss/TOC34.XML" ;; IEEE Transactions on Pattern Analysis and Machine Intelligence	  "https://ieeexplore.ieee.org/rss/TOC6221036.XML" ;; IEEE Transactions on Cybernetics	  "https://ieeexplore.ieee.org/rss/TOC6221021.XML" ;; IEEE Transactions on Systems, Man, Cybernetics: Systems	  "https://www.tandfonline.com/feed/rss/rirs20" ;; International Review of Sports and Exercise Psychology	  "https://iopscience.iop.org/journal/rss/1748-3190" ;; Bioinspiration and Biomimetics	  "http://feeds.nature.com/natrevmats/rss/current" ;; Nature Reviews Materials	  )	)  );; org-transclusion;; elegant-agenda-mode;;(use-package elegant-agenda-mode ;; :hook org-agenda-mode-hook);; projectile(use-package projectile  :ensure t  :init  (projectile-mode +1)  :config  (setq projectile-project-search-path '("~/org/projects/"))  (setq projectile-auto-discover nil)  :bind (:map projectile-mode-map	      ("C-c p" . projectile-command-map)));; ConTeXt(setq ConTeXt-Mark-version "IV")(add-hook 'ConTeXt-mode-hook          (lambda()            (setq TeX-command-default "ConTeXt Full")));;; Custom functions;; Formatter funtion for org-mode clocktable dynamic block(defun my-clocktable-write (&rest args)  "Custom clocktable writer. Uses the default writer but shifts the first column right."  (apply #'org-clocktable-write-default args)  (save-excursion    (forward-char) ;; move into the first table field    (org-table-move-column-right)    (org-table-move-column-right)    (org-table-move-column-right)    (org-table-move-column-right)    (org-table-move-column-right)    (org-table-move-column-right)    (org-table-move-column-right)))